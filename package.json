{
  "name": "react-bulma-ui-kit",
  "version": "1.5.1",
  "description": "UI kit based on bulma css framework for react",
  "repository": {
    "type": "git",
    "url": "https://github.com/axules/react-bulma-ui-kit"
  },
  "main": "./dist/components",
  "files": [
    "src",
    "dist",
    "LICENSE.md",
    "README.md"
  ],
