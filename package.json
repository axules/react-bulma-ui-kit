  "devDependencies": {
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.4",
    "@babel/eslint-parser": "^7.24.1",
    "@babel/plugin-proposal-export-default-from": "^7.22.17",
    "@babel/plugin-proposal-pipeline-operator": "^7.22.15",
    "@babel/preset-env": "^7.24.4",
    "@babel/preset-react": "^7.22.15",
