<head>
    <script>
      window.__SHEETS__=JSON.parse('<%= htmlWebpackPlugin.options.SHEETS_LIST %>');
    </script>
</head>
<body>
    <div id="general"></div>

    <% Array.prototype.forEach.call(htmlWebpackPlugin.files.css, function(file) { %>
